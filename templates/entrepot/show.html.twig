{% extends 'base.html.twig' %}

{% block title %}Entrepot{% endblock %}

{% block body %}
    <h1>Entrepot</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entrepot.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entrepot.name }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ entrepot.phone }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ entrepot.createdAt ? entrepot.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ entrepot.updatedAt ? entrepot.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Dateborn</th>
                <td>{{ entrepot.dateborn ? entrepot.dateborn|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Lieunaissance</th>
                <td>{{ entrepot.lieunaissance }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ entrepot.sexe }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ entrepot.adresse }}</td>
            </tr>
            <tr>
                <th>ImageFilename</th>
                <td>{{ entrepot.imageFilename }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('entrepot_index') }}">back to list</a>

    <a href="{{ path('entrepot_edit', {'id': entrepot.id}) }}">edit</a>

    {{ include('entrepot/_delete_form.html.twig') }}
{% endblock %}
